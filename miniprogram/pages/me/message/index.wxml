
<view wx:if="{{messages==null || messages.length==0}}">
  <van-empty
  class="custom-image"
  image="https://img.yzcdn.cn/vant/custom-empty-image.png"
  description="你还没有消息通知哟"
/>
</view>
<view wx:else style="background-color:rgb(222,222,222);min-height: 100vh;">

  <view wx:for="{{messages}}">

    <view class="time">
      {{item.send_time}}
    </view>

    <view class="card">

      <view class="title">
        {{item.message_type}}
      </view>

      <view class="content">

        <view class="left">
          消息编号
        </view>

        <view class="right">
          {{item.message_id}}
        </view>


      </view>

      <view class="content">

        <view class="left">
          消息内容
        </view>

        <view class="right">
          {{item.content}}
        </view>


      </view>

    </view>



  </view>

</view>