<!-- 未登录 -->
<view wx:if="{{!user}}">
  <van-empty description="请先登录" />
  <button bindtap="login">登录</button>
</view>

<!-- 已登录 -->
<view wx:else>
  <!-- 共有部分 -->

  <!-- 顶部框架 -->
<view class="{{user_type}}">

  <!-- 头像 -->
  <van-image class="v0" width="120rpx" height="120rpx" round src="{{user.avatarUrl}}" />

  <!-- 昵称 -->
  <view class="v1">
  <view class="v2">{{user.nickName}}({{ user_type=='tenant'? '租客':'房东'}})</view>

  <!-- 消息通知和切换 -->
  <view class="v3"> 
    <text bindtap="change">切换</text> 
  </view>
  </view>

</view>

<van-cell title="个人资料" is-link bindtap="jump1"></van-cell>

<van-cell title="消息通知" is-link>
  <van-icon  name="chat" info="9" class="custom-icon" />
</van-cell>

  <!-- 租客部分 -->
  <view wx:if="{{user_type=='tenant'}}">
    <van-cell title="我的订单" is-link></van-cell>
    <van-cell title="我的收藏" is-link></van-cell>
  </view>

  <!-- 房东部分 -->
  <view wx:else>
    <van-cell title="我的订单" is-link></van-cell>
    <van-cell title="我的收藏" is-link></van-cell>
  </view>

  <!-- 管理员部分 -->
  <view wx:if="{{root}}">

    <van-cell title="我审核的" is-link></van-cell>
    <van-cell title="审核房源" is-link></van-cell>
  </view>

  
  </view>