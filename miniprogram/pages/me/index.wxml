<text wx:else>房东模式</text> -->

  <!-- <van-button round bind:click="change"
size="mini">切换</van-button> -->




  <!-- 租客部分 -->
  <view wx:if="{{user_type=='tenant'}}">

    <van-cell title="个人资料" is-link></van-cell>
    <van-cell title="我的订单" is-link></van-cell>
    <van-cell title="我的收藏" is-link></van-cell>

  </view>

  <!-- 房东部分 -->
  <view wx:else>
  </view>

  <!-- 管理员部分 -->
  <view wx:if="{{root}}">

    <van-cell title="我审核的" is-link></van-cell>
    <van-cell title="审核房源" is-link></van-cell>
  </view>

  
  </view>